<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_hr_expense" model="ir.sequence">
            <field name="name">HR Expense Sequence</field>
            <field name="code">hr.expense</field>
            <field name="prefix">EX/24/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_hr_expense_sheet" model="ir.sequence">
            <field name="name">Expense Sequence Summary</field>
            <field name="code">hr.expense.sheet</field>
            <field name="prefix">ES/24/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
